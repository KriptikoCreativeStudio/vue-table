{
  "name": "@kriptiko/vue-table",
  "description": "Vue table component for rendering server-side data.",
  "version": "1.0.1",
  "license": "MIT",
  "private": false,
  "author": "José Osório",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KriptikoCreativeStudio/vue-table.git"
  },
  "keywords": [
    "vue-table",
    "pagination",
    "search",
    "sorting",
    "filters",
    "reorderable",
